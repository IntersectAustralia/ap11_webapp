- if user_signed_in?
  %td
    %li.dropdown
      = link_to user_dropdown_menu, '#', :class => "dropdown-toggle", 'data-toggle' => "dropdown"
      %ul.dropdown-menu
        %li= link_to "Logout", destroy_user_session_path
        %li= link_to "Change My Password", users_edit_password_path
        %li= link_to "Edit My Details", edit_user_registration_path
    %a{:class => "btn btn-primary", :href => "/"}
      %i{:class => "icon-th-list icon-white"}
      Home
    %a{:class => "btn btn-primary", :href => experiments_path}
      %i{:class => "icon-th-list icon-white"}
      View Experiments
    %a{:class => "btn btn-primary", :href => users_path}
      %i{:class => "icon-user icon-white"}
      View Users
    %a{:class => "btn btn-primary", :href => access_requests_users_path}
      %i{:class => "icon-user icon-white"}
      View Access Request

- else
  %td
    %a{:class => "btn btn-primary", :href => "/"}
      %i{:class => "icon-th-list icon-white"}
      Home
    %a{:class => "btn btn-primary", :href => "/"}
      %i{:class => "icon-th-list icon-white"}
      Login/Sign up
